{
  "url": "http://yoursite.com",
  "title": "My Octopress Blog",
  "subtitle": "A blogging framework for hackers.",
  "author": "Your Name",
  "simple_search": "https://www.google.com/search",
  "description": null,
  "date_format": "ordinal",
  "subscribe_rss": "/atom.xml",
  "subscribe_email": null,
  "email": null,
  "root": "/",
  "permalink": "/blog/:year/:month/:day/:title/",
  "source": "source",
  "destination": "public",
  "plugins": "plugins",
  "code_dir": "downloads/code",
  "category_dir": "blog/categories",
  "markdown": "rdiscount",
  "rdiscount": {
    "extensions": [
      "autolink",
      "footnotes",
      "smart"
    ]
  },
  "highlighter": "pygments",
  "paginate": 10,
  "paginate_path": "posts/:num",
  "recent_posts": 5,
  "excerpt_link": "Read on &rarr;",
  "excerpt_separator": "<!--more-->",
  "default_asides": [
    "asides/recent_posts.hbs",
    "asides/github.hbs",
    "asides/delicious.hbs",
    "asides/pinboard.hbs",
    "asides/googleplus.hbs"
  ],
  "github_user": null,
  "github_repo_count": 0,
  "github_show_profile_link": true,
  "github_skip_forks": true,
  "twitter_user": null,
  "twitter_tweet_button": true,
  "google_plus_one": false,
  "google_plus_one_size": "medium",
  "googleplus_user": null,
  "googleplus_hidden": false,
  "pinboard_user": null,
  "pinboard_count": 3,
  "delicious_user": null,
  "delicious_count": 3,
  "disqus_short_name": null,
  "disqus_show_comment_count": false,
  "google_analytics_tracking_id": null,
  "facebook_like": false
}
